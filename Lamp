using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class Electric_Lamp : Electric_Switch {


// UPDATE
	void Update () 
	{
		if(breakerActive)
		{
			if(switchActive)
			{
				gameObject.SetActive (true);
			}
			else
			{
				gameObject.SetActive (false);
			}
		}
		else
		{
			gameObject.SetActive (false);
		}
	}
}
